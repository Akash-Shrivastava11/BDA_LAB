create keyspace library with replication={ 'class':'SimpleStrategy','replication_factor':1};
create table library_info (stud_id int , Counter_value counter,Stud_name varchar,Book_name text,Book_id int,Date_of_issue date,PRIMARY KEY(stud_id,Stud_name,Book_name,Book_id,Date_of_issue));
update library_info set counter_value=counter_value+1 where stud_id=1 and stud_name='Ankit' and book_name='BDA' and book_id=123 and date_of_issue='2020-06-11';
cqlsh:library> select * from library_info;
update library_info set counter_value=counter_value+1 where stud_id=2 and stud_name='Saqib' and book_name='Pandas' and book_id=179 and date_of_issue='2020-08-16';
cqlsh:library> select * from library_info;
select * from library_info where stud_id=2;
copy library_info(stud_id,stud_name,book_name,book_id,date_of_issue) to '/home/bmsce/Desktop/lab2.csv';
